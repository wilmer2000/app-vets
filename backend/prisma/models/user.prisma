model User {
  userId String @id @default(uuid())

  isActive Boolean @default(false)
  role     Role    @default(USER)

  email    String  @unique
  password String?
  name     String?
  lastname String?

  clients Client[]
  staff   Staff[]

  contact Contact?
  address Address?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
