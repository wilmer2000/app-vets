model Entity {
  entityId String @id @default(uuid())

  name        String
  description String

  clients      Client[]
  staff        Staff[]
  services     Service[]
  appointments Appointment[]

  contact       Contact?
  address       Address?
  configuration Configuration?

  isActive  Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
